@model ChatWebApp.Models.Rank

@{
	ViewData["Title"] = "Details";
}

<div class="default">

	<h3 class="header-text">@Html.DisplayFor(model => model.Username)'s rank</h3>

	<div>
		<hr />
		<dl class="row">
			<dt class="col-sm-2 def-text">
				@Html.DisplayNameFor(model => model.NumeralRank)
			</dt>
			<dd class="col-sm-10 def-text to-left">
				@Html.DisplayFor(model => model.NumeralRank)
			</dd>
			<dt class="col-sm-2 def-text ">
				@Html.DisplayNameFor(model => model.Feedback)
			</dt>
			<dd class="col-sm-10 def-text to-left">
				@Html.DisplayFor(model => model.Feedback)
			</dd>
			<dt class="col-sm-2 def-text">
				@Html.DisplayNameFor(model => model.SubmitTime)
			</dt>
			<dd class="col-sm-10 def-text to-left">
				@Html.DisplayFor(model => model.SubmitTime)
			</dd>
		</dl>
	</div>
	<div>
		<a asp-action="Edit" asp-route-Username="@Model?.Username">Edit</a> |
		<a asp-action="Index">Back to List</a>
	</div>
</div>